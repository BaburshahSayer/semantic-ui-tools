{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "modules": false,
        "targets": {
          "browsers": [
            "last 8 versions",
            "safari > 8",
            "firefox > 23",
            "chrome > 24",
            "opera > 15",
            "not ie < 11",
            "not ie_mob <= 11"
          ]
        }
      }
    ]
  ]
}
